# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Solana RPC Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
# Alternative RPC endpoints (choose one):
# SOLANA_RPC_URL=https://solana-api.projectserum.com
# SOLANA_RPC_URL=https://rpc.ankr.com/solana

# Helius WebSocket URL (for real-time transaction monitoring)
HELIUS_WS_URL=wss://atlas-mainnet.helius-rpc.com
# Get your Helius API key from: https://www.helius.xyz/
# Replace with your actual Helius WebSocket URL

# Security Configuration
ENCRYPTION_KEY=your-32-character-encryption-key-here!!!
# Generate a secure 32-character key for encrypting private keys
# You can use: openssl rand -base64 32

# Optional: Development Settings
NODE_ENV=production
# Set to 'development' for development mode

# Optional: Database Configuration (if using external database in future)
# DATABASE_URL=your_database_url_here

# Optional: Logging Configuration
LOG_LEVEL=info
# Options: error, warn, info, debug

# Optional: Performance Settings
MAX_CONCURRENT_TRADES=3
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=30

# Optional: Jupiter API Configuration (defaults should work)
# JUPITER_API_URL=https://quote-api.jup.ag/v6